        "storybook": "^7.6.15",
        "storybook-dark-mode": "3.0.3",
        "storybook-react-i18next": "2.0.10",
        "stylelint": "16.2.1",
        "stylelint-config-idiomatic-order": "10.0.0",
        "stylelint-config-standard": "36.0.0",
        "stylelint-config-tailwindcss": "0.0.7",
        "stylelint-order": "6.0.4",
